{
  "name": "animepaste",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/XLorPaste/AnimePaste#readme",
  "bugs": {
    "url": "https://github.com/XLorPaste/AnimePaste/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XLorPaste/AnimePaste.git"
  },
  "license": "MIT",
  "author": "XLor",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "anime": "./cli.mjs"
  },
  "files": [
    "dist",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "format": "prettier --write src/**/*.ts",
    "release": "bumpp --commit --push --tag && pnpm release",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "preversion": "pnpm build"
  },
  "dependencies": {
    "breadc": "^0.4.1",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "unbuild": "^0.7.4"
  },
  "packageManager": "pnpm@7.3.0"
}
