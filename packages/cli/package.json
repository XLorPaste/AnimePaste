{
  "name": "animespace",
  "version": "0.0.7",
  "description": "Create your own Anime Space",
  "keywords": [
    "anime",
    "animespace",
    "cli"
  ],
  "homepage": "https://anime.docs.xlorpaste.cn/",
  "bugs": {
    "url": "https://github.com/XLorPaste/AnimePaste/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XLorPaste/AnimePaste.git"
  },
  "license": "AGPL-3.0",
  "author": "XLor",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "anime": "./cli.mjs"
  },
  "files": [
    "dist",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "format": "prettier --write {src,test}/**/*.ts",
    "test": "vitest",
    "test:ci": "vitest --run",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "preversion": "pnpm build"
  },
  "dependencies": {
    "@animespace/animegarden": "workspace:*",
    "@animespace/core": "workspace:*",
    "@animespace/download": "workspace:*",
    "@breadc/color": "^0.9.3",
    "@onekuma/map": "^0.1.9",
    "breadc": "^0.9.3",
    "cli-progress": "^3.12.0",
    "date-fns": "^2.30.0",
    "death": "^1.1.0",
    "debug": "^4.3.4",
    "fastest-levenshtein": "^1.0.16",
    "fs-extra": "^11.1.1",
    "open-editor": "^4.0.0",
    "prompts": "^2.4.2",
    "undici": "^5.22.0",
    "webtorrent": "^2.0.16"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/death": "^1.1.2",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/prompts": "^2.4.4",
    "@types/webtorrent": "^0.109.3",
    "bangumi-data": "^0.3.115",
    "unbuild": "^1.2.1",
    "unbuild-sea": "^0.0.5"
  },
  "engines": {
    "node": ">=v16.0.0"
  }
}