{
  "name": "@animepaste/database",
  "version": "0.0.2",
  "description": "Local SQLite database used for Anime Paste CLI",
  "keywords": [
    "anime",
    "AnimePaste",
    "XLorPaste",
    "cli"
  ],
  "homepage": "https://github.com/XLorPaste/AnimePaste#readme",
  "bugs": {
    "url": "https://github.com/XLorPaste/AnimePaste/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XLorPaste/AnimePaste.git"
  },
  "license": "AGPL-3.0",
  "author": "XLor",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "prisma",
    "*.mjs"
  ],
  "scripts": {
    "build": "prisma generate && unbuild",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "dev": "unbuild --stub",
    "postinstall": "prisma generate",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "axios": "^0.27.2",
    "cnchar": "^3.2.2",
    "cnchar-trad": "^3.2.2",
    "date-fns": "^2.29.3",
    "debug": "^4.3.4",
    "node-html-parser": "^6.1.1"
  },
  "devDependencies": {
    "prisma": "^4.4.0",
    "unbuild": "^0.8.11"
  },
  "engines": {
    "node": ">=v16.17.0"
  }
}
