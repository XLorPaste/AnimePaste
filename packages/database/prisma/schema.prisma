datasource db {
  provider = "sqlite"
  url      = "file:./anime.db"
}

generator client {
  provider = "prisma-client-js"
}

model Resource {
  id        Int       @id @default(autoincrement())
  link      String    @unique
  type      String
  title     String    @unique
  fansub    String
  magnet    String
  createdAt DateTime 
}