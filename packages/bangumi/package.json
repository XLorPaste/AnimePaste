{
  "name": "@animepaste/bangumi",
  "version": "0.0.5",
  "description": "Bangumi List",
  "keywords": [
    "anime",
    "AnimePaste",
    "XLorPaste",
    "cli"
  ],
  "homepage": "https://github.com/XLorPaste/AnimePaste#readme",
  "bugs": {
    "url": "https://github.com/XLorPaste/AnimePaste/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XLorPaste/AnimePaste.git"
  },
  "license": "MIT",
  "author": "XLor",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./types.d.ts"
    },
    "./recent": {
      "require": "./dist/recent.cjs",
      "import": "./dist/recent.mjs",
      "types": "./types.d.ts"
    },
    "./data/*": "./data/*",
    "./bgm": {
      "require": "./dist/bgm.cjs",
      "import": "./dist/bgm.mjs",
      "types": "./dist/bgm.d.ts"
    },
    "./transform": {
      "require": "./dist/transform.cjs",
      "import": "./dist/transform.mjs",
      "types": "./dist/transform.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs",
      "types": "./dist/vite.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./types.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "bin": {
    "bangumi": "./cli.mjs"
  },
  "files": [
    "data",
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "before:version": "vitest --run && pnpm typecheck && pnpm build",
    "build": "unbuild && pnpm build:data",
    "build:data": "optc scripts/build.ts",
    "dev": "unbuild --stub",
    "release": "release-it",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "breadc": "^0.6.6",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "bangumi-data": "^0.3.102",
    "date-fns": "^2.29.3",
    "optc": "^0.5.3",
    "unbuild": "^0.9.4",
    "vite": "^3.1.8"
  },
  "peerDependencies": {
    "bangumi-data": "^0.3.102",
    "vite": "^2.9.14 || ^3.0.4"
  },
  "peerDependenciesMeta": {
    "bangumi-data": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=v16.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(bangumi): release v${version}",
      "tag": false,
      "push": true
    },
    "npm": {
      "publish": true,
      "publishArgs": [
        "--access",
        "public"
      ]
    }
  }
}
