{
  "name": "@animespace/animegarden",
  "version": "0.0.7",
  "description": "Create your own Anime Space",
  "keywords": [
    "anime",
    "animegarden",
    "dmhy",
    "animespace",
    "cli"
  ],
  "homepage": "https://anime.docs.xlorpaste.cn/",
  "bugs": {
    "url": "https://github.com/XLorPaste/AnimePaste/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XLorPaste/AnimePaste.git"
  },
  "license": "AGPL-3.0",
  "author": "XLor",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@animespace/core": "workspace:*",
    "@breadc/color": "^0.9.3",
    "@onekuma/map": "^0.1.10",
    "animegarden": "^0.0.14",
    "anitomy": "^0.0.14",
    "bgmc": "^0.0.4",
    "cli-progress": "^3.12.0",
    "date-fns": "^2.30.0",
    "defu": "^6.1.2",
    "fs-extra": "^11.1.1",
    "libaria2": "^1.0.95",
    "undici": "^5.22.0",
    "webtorrent": "^2.0.18"
  },
  "devDependencies": {
    "@types/webtorrent": "^0.109.3"
  },
  "engines": {
    "node": ">=v16.0.0"
  }
}