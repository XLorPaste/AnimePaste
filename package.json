{
  "name": "@animepaste/app",
  "private": true,
  "scripts": {
    "build": "vite-ssg build",
    "build:all": "pnpm build:cli && pnpm build",
    "build:cli": "pnpm -C packages/cli build",
    "dev": "vite",
    "dev:worker": "wrangler pages dev --kv ANIME --binding DEV=true -- vite",
    "format": "prettier --write src/**/*.{ts,vue} packages/**/*.ts --ignore-path .gitignore",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit -p package.json --resolveJsonModule --skipLibCheck"
  },
  "dependencies": {
    "@vueuse/core": "^8.7.5",
    "@vueuse/head": "^0.7.6",
    "bangumi-data": "^0.3.91",
    "date-fns": "^2.28.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "plyr": "^3.7.2",
    "scrollreveal": "^4.0.9",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.6",
    "@types/node": "^18.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/prompts": "^2.4.0",
    "@types/scrollreveal": "^0.0.8",
    "@unocss/reset": "^0.41.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.41.1",
    "unplugin-auto-import": "^0.9.1",
    "unplugin-icons": "^0.14.6",
    "unplugin-vue-components": "^0.20.1",
    "vite": "^2.9.13",
    "vite-plugin-info": "^0.1.0",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-pages": "^0.24.3",
    "vite-plugin-pwa": "^0.12.0",
    "vite-ssg": "^0.20.1",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.2",
    "wrangler": "^2.0.15"
  },
  "packageManager": "pnpm@7.4.0"
}
