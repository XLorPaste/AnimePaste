{
  "name": "@animepaste/app",
  "private": true,
  "scripts": {
    "build": "vite-ssg build",
    "build:all": "pnpm build:client && pnpm build:cli && pnpm build",
    "build:cli": "pnpm -C packages/cli build",
    "build:client": "pnpm -C packages/client build",
    "dev": "vite",
    "dev:worker": "wrangler pages dev ./dist --kv ANIME --binding DEV=true",
    "format": "prettier --write src/**/*.{ts,vue} packages/**/*.ts --ignore-path .gitignore",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit -p package.json --resolveJsonModule --skipLibCheck"
  },
  "dependencies": {
    "@animepaste/client": "workspace:*",
    "@vueuse/core": "^8.7.5",
    "@vueuse/head": "^0.7.6",
    "bangumi-data": "^0.3.91",
    "date-fns": "^2.28.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "plyr": "^3.7.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.13.0",
    "@iconify-json/carbon": "^1.1.5",
    "@types/node": "^18.0.0",
    "@types/nprogress": "^0.2.0",
    "@types/prompts": "^2.0.14",
    "@unocss/reset": "^0.39.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.39.3",
    "unplugin-auto-import": "^0.9.0",
    "unplugin-icons": "^0.14.5",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^2.9.12",
    "vite-plugin-info": "^0.0.1",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-pages": "^0.24.2",
    "vite-ssg": "^0.20.1",
    "vitest": "^0.15.2",
    "vue-tsc": "^0.38.1",
    "wrangler": "^2.0.15"
  },
  "packageManager": "pnpm@7.3.0"
}
